<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net48</TargetFramework>
    <SccProjectName>SAK</SccProjectName>
    <SccLocalPath>SAK</SccLocalPath>
    <SccAuxPath>SAK</SccAuxPath>
    <SccProvider>SAK</SccProvider>
    <AssemblyTitle>IGWebApiClient</AssemblyTitle>
    <Company>IG Group</Company>
    <Product>IGWebApiClient</Product>
    <NeutralLanguage>en</NeutralLanguage>
    <Description>IG Public API for .NET platforms</Description>
    <AssemblyVersion>1.5.29.0</AssemblyVersion>
    <FileVersion>1.5.29.0</FileVersion>
    <OutputPath>bin\$(Configuration)\</OutputPath>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Net.Http" Version="2.2.29" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    <PackageReference Include="PCLCrypto" Version="2.0.147" />
    <PackageReference Include="Validation" Version="2.4.22" />
    <PackageReference Include="Lightstreamer.DotNet.Client" Version="3.1.6640.22649" />
  </ItemGroup>
</Project>